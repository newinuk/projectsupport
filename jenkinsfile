pipeline {
    agent any
    stages {
         stage('clone') {
            steps {
                git branch: 'main', url: 'https://github.com/sankardevvinu/projectsupport.git'
            }
         }
        
         stage('Build') {
	      steps {
		  sh "docker build -t sankardev/task:tagname ."
	    }
	 }
 }	 
	 
